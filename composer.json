{
  "name": "sharpapi/laravel-job-positions-api",
  "description": "Job Positions API for Laravel powered by SharpAPI.com",
  "keywords": [
    "sharpapi",
    "api",
    "api integration",
    "restful api",
    "php",
    "laravel",
    "software development",
    "job positions",
    "job database",
    "job titles",
    "job categories",
    "related jobs",
    "job description",
    "hr",
    "recruitment"
  ],
  "homepage": "https://github.com/sharpapi/laravel-job-positions-api",
  "license": "MIT",
  "authors": [
    {
      "name": "Dawid Makowski",
      "email": "contact@sharpapi.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
    "sharpapi/php-core": "^1.0",
    "spatie/url": "^2.4"
  },
  "require-dev": {
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "SharpAPI\\JobPositionsApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SharpAPI\\JobPositionsApi\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": "",
    "build": [
    ],
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@composer run build"
    ],
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SharpAPI\\JobPositionsApi\\JobPositionsApiProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}